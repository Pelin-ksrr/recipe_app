{
  "expo": {
    "name": "Special Recipes",
    "slug": "recipe_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/recipes.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/recipes.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pelin31.recipe_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/recipes.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/recipes.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "79655ecc-a837-48f7-9f16-7d7693a8a264"
      }
    }
  }
}
